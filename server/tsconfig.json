{
    "extends": "../tsconfig.json",
    "exclude": ["node_modules"],
    "compilerOptions": {
        "rootDir": "./src",
        "lib": ["dom", "dom.iterable", "esnext"],
        "target": "es2016",
        "module": "commonjs",
        "noEmit": false,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist"
    },
    "include": ["./src"]
}
