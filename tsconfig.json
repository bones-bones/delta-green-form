{
    "compilerOptions": {
        "target": "ES2016",
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "moduleResolution": "node",
        "sourceMap": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true
    }
}
